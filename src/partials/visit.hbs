{{!-- VISIT PAGE --}}
<section id="visit" class="visit-page page">

	<div class="container">
	{{!-- MENU --}}
	  	<div class="menu_wrapper">
			<dl class="menu">
				{{#each site.menu }}
				<dd {{#is this.name "visit"}}class="active"{{/is}}>
					<a href="{{this.link}}">{{this.name}}</a>
				</dd>
				{{/each}}
			</dl>
		</div>
	{{!-- /MENU --}}

		<h2>{{ visit.title }}</h2>
	
		<div class="row">
		
			<div class="col-sm-6 col-xs-12">
				
			</div>
			<div class="col-sm-6 col-xs-12">
				
			</div>
			
		</div>
	</div>

</section>